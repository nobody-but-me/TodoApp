
#include <objects.h>
#include <raylib.h>
#include <iostream>


void OBJECT::Button() {
    self._position = (Vector2){100, 100};
    self._size     = (Vector2){100, 100};
    return;
}

void OBJECT::_render() {
    // DrawRectangleV(Vector2 position, Vector2 size, Color color)
    DrawRectangleV(self._position, self._size, RAY_WHITE);
}

